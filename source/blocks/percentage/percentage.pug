mixin percentage(percent)
    -let a = percent*295/100, b = 295 - a, dasharray = `${a} ${b}`;
    svg.percentage
        circle.percentage__circle(r="45", cx="50%", cy="50%", stroke-dasharray= dasharray)
        text.percentage__text(text-anchor="middle", x="50%", y="64%")= percent >= 101 ? 'error' : percent